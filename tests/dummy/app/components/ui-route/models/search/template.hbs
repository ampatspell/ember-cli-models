<div class="row">
  {{input value=id}}
</div>

<div class="row">
  {{ui-block/json loader.state}}
</div>

{{#if loader.autoload.isLoading}}

  <div class="row">
    Loading '{{id}}'â€¦
  </div>

{{else if loader.isError}}

  <div class="row">
    {{ui-block/json loader.error}}
  </div>

{{else}}

  <div class="row">
    {{model}}
  </div>

  <div class="row">
    {{ui-block/json model.serialized}}
  </div>

{{/if}}
